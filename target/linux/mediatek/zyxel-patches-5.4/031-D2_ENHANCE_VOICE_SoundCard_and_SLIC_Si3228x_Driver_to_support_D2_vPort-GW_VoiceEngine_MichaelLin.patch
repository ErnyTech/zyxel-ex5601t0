+
+/*
+** Calibration Constants
+*/
+#define SI3228X_CAL_STD_CALR1               0xC0    /* FF */
+#define SI3228X_CAL_STD_CALR2               0x18    /* F8 */
+
+/* Timeouts in 10s of ms */
+#define SI3228X_TIMEOUT_DCDC_UP             200
+#define SI3228X_TIMEOUT_DCDC_DOWN           200
