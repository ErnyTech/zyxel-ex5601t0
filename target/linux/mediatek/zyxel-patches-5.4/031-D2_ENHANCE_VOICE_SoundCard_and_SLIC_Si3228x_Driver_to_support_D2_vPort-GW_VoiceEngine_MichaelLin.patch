+int Si3228x_PCMSetup (proslicChanType *pProslic, int preset)
+{
+  uInt8 regTemp;
+  uInt8 pmEn;
+
+  pmEn = ReadReg(pProHW,pProslic->channel,PMCON) & 0x01; /* PM/wideband lockout */
+  if (Si3228x_PCM_Presets[preset].widebandEn && pmEn)
+  {
