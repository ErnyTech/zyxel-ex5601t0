-static uInt32 Isqrt32(uInt32 num)
-{
-  uInt32 one,res,op;
-
-  op=num;
-  res=0;
-  one = 1L << 30;
-  while(one > op)
-  {
-    one >>=2;
-  }
-
-  while(one !=0)
-  {
-    if(op >= res + one)
-    {
-      op = op - (res + one);
-      res = res + 2*one;
-    }
-    res >>= 1;
-    one >>= 2;
-  }
-
-  return (res);
-}
