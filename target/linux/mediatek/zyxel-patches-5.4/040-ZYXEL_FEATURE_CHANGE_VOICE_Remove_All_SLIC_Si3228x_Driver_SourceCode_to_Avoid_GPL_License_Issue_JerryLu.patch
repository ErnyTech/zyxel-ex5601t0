-
-int ProSLIC_testInDCFeed(proslicChanType *pProslic,
-                         proslicTestInObjType *pTstin)
-{
-  uInt8 enhanceRegSave;
-  proslicMonitorType monitor;
-  ramData lcroffhk_save = 0;
