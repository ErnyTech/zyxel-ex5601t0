-          ramData patchData;
-          patchData = pProslic[k]->ReadRAMX(pProslic[k]->pProHWX, pProslic[k]->channel,
-                                            PROSLIC_RAM_PATCHID);
-
-          if( patchData ==  patch->patchSerial)
-          {
-            data = 0;
-          }
-          else
-          {
-            init_opt = INIT_NO_OPT;
-          }
-        } /* SOFTRESET */
-
-        if(data == 1)
-        {
-          ProSLIC_LoadPatch_extended(pProslic[k],patch, FALSE, (k&1));
