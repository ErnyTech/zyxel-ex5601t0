-        regData = ReadReg(pVdaaHW,pVdaa->channel,PCM_SPI_CTRL);
-        WriteReg(pVdaaHW,pVdaa->channel,PCM_SPI_CTRL, regData & ~(0x80));
-      }
-      break;
