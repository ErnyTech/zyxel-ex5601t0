-    return RC_TEST_DISABLED;
-  }
-
-  /* Return if already disabled */
-  if(!pTstin->pcmLpbkTest.pcmLpbkEnabled)
-  {
-    return RC_NONE;
-  }
-
-  /* Disable PCM Bus */
-  regData = ProSLIC_ReadReg(pProslic,11); /* PCMMODE */
-  ProSLIC_WriteReg(pProslic,11,regData &= ~0x10);
