-#ifdef SI3219X
-      || ( IS_SI3219X(hProslic->deviceId) )
-#endif
-     )
-  {
-    return ProSLIC_GetInterruptHelper(hProslic, interruptMap, pIntData, 1);
-  }
-#endif
-#if defined (SI3226X) || defined (SI3228X)
-  if( 0
-#ifdef SI3226X
-      || (hProslic->deviceId->chipType >= SI32260
-          && hProslic->deviceId->chipType <= SI32269)
