-/*****************************************************************************************************/
-/* RAM Write wrapper */
-int proslic_write_ram(void *hCtrl, uInt8 channel, uInt16 ramAddr, ramData data )
-{
-
-  ramData myData = data;
-  int rc = 0;
-  //unsigned long irqSettings;
-  //struct spi_device *spi_data = (struct spi_device *)hCtrl;
