-int ProSLIC_testInPCMLpbkDisable(proslicChanType *pProslic,
-                                 proslicTestInObjType *pTstin)
-{
-  uInt8 regData;
-
-  /* Valid device check */
-  if(TSTIN_INVALID_PART_NUM)
-  {
-    return RC_UNSUPPORTED_FEATURE;
-  }
-
-  /* Check if enabled */
-  if(!pTstin->pcmLpbkTest.testEnable)
