-#if PROSLIC_XFER_BUF == 2 /* 16 bit mode */
-  for(i = 0; i < 4; i+=2)
-#else /* Byte by byte */
-  for(i = 0; i < 3; i++)
-#endif
-  {
-    spi_write(proslic_spidev, &data_stream[i], PROSLIC_XFER_BUF);
-  }
-#endif  
-
-  return PROSLIC_SPI_OK;
