-void ProSLIC_PSTN_delay_poll(proslicTestStateType *pState, uInt16 delay)
-{
-  uInt16 delayCount;
-
-  if((delay/PSTN_DET_POLL_RATE) < 2)
-  {
-    delayCount = 0;
-  }
-  else
-  {
-    delayCount = (delay/PSTN_DET_POLL_RATE) - 2;
-  }
-
-  pState->waitIterations++;
