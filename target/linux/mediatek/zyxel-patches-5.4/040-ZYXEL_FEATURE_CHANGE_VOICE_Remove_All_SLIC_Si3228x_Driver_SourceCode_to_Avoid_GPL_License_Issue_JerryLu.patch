-    kfree(proslic_data);
-    return -EIO;
-  }
-#endif
-  spi_set_drvdata(spi, proslic_data);
-
-  /* Probe to determine the number of DAA's or ProSLIC's present */
-  for(channel = 0; channel < proslic_channel_count; channel++)
