-  pChan->deviceId->ctrlInterface->Reset_fptr(
-    pChan->deviceId->ctrlInterface->hCtrl,0);
-
-  pChan->deviceId->ctrlInterface->Delay_fptr(
-    pChan->deviceId->ctrlInterface->hTimer,250);
