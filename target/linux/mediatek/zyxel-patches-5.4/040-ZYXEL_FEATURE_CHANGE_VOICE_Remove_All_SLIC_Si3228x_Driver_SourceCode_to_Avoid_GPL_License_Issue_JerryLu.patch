-
-    case LPBK_IDL:
-      if(lpbk_status)
-      {
-        regData = ReadReg(pVdaaHW,pVdaa->channel,CTRL1);
-        WriteReg(pVdaaHW,pVdaa->channel,CTRL1, regData | 0x02);
-      }
-      else
-      {
-        regData = ReadReg(pVdaaHW,pVdaa->channel,CTRL1);
-        WriteReg(pVdaaHW,pVdaa->channel,CTRL1, regData & ~(0x02));
-      }
-      break;
