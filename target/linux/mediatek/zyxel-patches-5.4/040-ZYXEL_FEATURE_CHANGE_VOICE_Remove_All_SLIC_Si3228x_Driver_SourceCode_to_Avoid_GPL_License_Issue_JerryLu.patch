-  if( init_opt != INIT_REINIT )
-  {
-    if( (SiVoice_IdentifyChannels(pProslic, size, &device_count, NULL) != RC_NONE)
-        ||(device_count == 0) )
-    {
-      DEBUG_PRINT(*pProslic, "%s: failed to detect any ProSLICs\n", LOGPRINT_PREFIX);
-      return RC_SPI_FAIL;
